{
  "containerDefinitions": [
    {
      "name": "bdd-api",
      "image": "954976315238.dkr.ecr.us-west-1.amazonaws.com/bdd-api",
      "cpu": 512,
      "memory": 1024,
      "portMappings": [
        {
          "name": "api-port",
          "containerPort": 80000,
          "hostPort": 80,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "essential": true,
      "environment": [],
      "environmentFiles": [],
      "mountPoints": [],
      "volumesFrom": [],
      "secrets": [
        {
          "name": "DB_HOST",
          "valueFrom": "arn:aws:ssm:us-west-1:954976315238:parameter/DB_HOST"
        },
        {
          "name": "DB_NAME",
          "valueFrom": "arn:aws:ssm:us-west-1:954976315238:parameter/DB_NAME"
        },
        {
          "name": "DB_PASSWORD",
          "valueFrom": "arn:aws:ssm:us-west-1:954976315238:parameter/DB_PASSWORD"
        },
        {
          "name": "DB_PORT",
          "valueFrom": "arn:aws:ssm:us-west-1:954976315238:parameter/DB_PORT"
        },
        {
          "name": "DB_USER",
          "valueFrom": "arn:aws:ssm:us-west-1:954976315238:parameter/DB_USER"
        }
      ],
      "ulimits": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/bdd-api-task",
          "mode": "non-blocking",
          "awslogs-create-group": "true",
          "max-buffer-size": "25m",
          "awslogs-region": "us-west-1",
          "awslogs-stream-prefix": "ecs"
        },
        "secretOptions": []
      },
      "systemControls": []
    }
  ],
  "family": "bdd-api-task",
  "taskRoleArn": "arn:aws:iam::954976315238:role/ecsTaskExecutionRole",
  "executionRoleArn": "arn:aws:iam::954976315238:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "volumes": [],
  "placementConstraints": [],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "512",
  "memory": "1024",
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  },
  "tags": []
}
